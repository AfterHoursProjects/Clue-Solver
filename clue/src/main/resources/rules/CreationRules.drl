package rules;

import model.Card;
import model.Room;
import model.Suspect;
import model.Weapon;
import model.TripleList;
import model.Triple;

rule "create all triples" 
	when
		$room:Room(  )
		$weapon:Weapon(  )
		$suspect:Suspect(  )
		$list:TripleList(  )
	then
		$list.addTriple($room, $suspect, $weapon);
end